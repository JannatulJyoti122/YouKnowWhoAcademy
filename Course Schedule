#include<bits/stdc++.h>
using namespace std;
const int N =1e5+9;
vector<int>g[N];
int indeg[N];
int main(){
    int n,m;
    cin>>n>>m;

    for(int i=0;i<m;i++){
        int u,v;
        cin>>u>>v;
        g[u].push_back(v);
        indeg[v]++;
    }
    vector<int>ord;
    queue<int>q;
    for(int i=1;i<=n;i++){
        if(indeg[i]==0){
            q.push(i);
        }
    }
    while(!q.empty()){
        int u=q.front();
        ord.push_back(u);
        q.pop();
        for(auto v:g[u]){
            indeg[v]--;
            if(indeg[v]==0) q.push(v);
        }
    }
    if(ord.size()<n){
        cout<<"IMPOSSIBLE\n";
    }
    else{
        for(int i=0;i<ord.size();i++) cout<<ord[i]<<" ";
        cout<<endl;
    }

}
