#include <bits/stdc++.h>
using namespace std;
using ll=long long int;
int main() {
    int n,m;
    cin>>n>>m;
    int new_n=n-m;
    if(n<m){
        cout<<0<<endl;
        return 0;
    }
    if(n==m){
        cout<<"infinity\n"; return 0;
    }
    int ct=0;
    for(int i=1;i*i<=new_n;i++){
        if(new_n%i==0){
            if(n%i==m)ct++;
            if(n%(new_n/i)==m&&i!=(new_n/i)) ct++;
        }
    }
   cout<<ct<<endl;
}
